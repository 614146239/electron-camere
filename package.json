{
  "name": "camere",
  "version": "1.0.6",
  "description": "An Electron application with Vue and TypeScript",
  "main": "./out/main/index.js",
  "author": "614146239",
  "homepage": "https://github.com/614146239/electron-camere",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts,.vue --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "vue-tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview ",
    "dev": "electron-vite dev --watch",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "npm run build && electron-builder --win --config --publish=always",
    "build:mac": "npm run build && electron-builder --mac --config",
    "build:linux": "npm run build && electron-builder --linux --config"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^1.0.3",
    "@electron-toolkit/utils": "^1.0.2",
    "@element-plus/icons-vue": "^2.1.0",
    "@tensorflow-models/face-detection": "^1.0.2",
    "@tensorflow-models/face-landmarks-detection": "^1.0.2",
    "@tensorflow/tfjs": "^4.9.0",
    "electron-updater": "^5.3.0",
    "element-plus": "^2.3.8",
    "pinia": "^2.1.4",
    "pinia-plugin-persist": "^1.0.0",
    "three": "^0.154.0",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@electron/notarize": "^1.2.4",
    "@rushstack/eslint-patch": "^1.3.2",
    "@types/node": "16.18.21",
    "@types/three": "^0.152.1",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "electron": "^25.3.1",
    "electron-builder": "^23.6.0",
    "electron-vite": "^1.0.25",
    "eslint": "^8.45.0",
    "eslint-plugin-vue": "^9.15.1",
    "less": "^4.1.3",
    "prettier": "^2.8.8",
    "typescript": "^4.9.5",
    "unplugin-auto-import": "^0.15.3",
    "unplugin-vue-components": "^0.24.1",
    "vite": "^4.4.4",
    "vue": "^3.3.4",
    "vue-tsc": "^1.8.5"
  },
  "build": {
    "appId": "https://github.com/614146239/electron-camere",
    "win": {
      "target": "nsis",
      "icon": "./resources/camera.png"
    },
    "directories": {
      "buildResources": "build"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  },
  "publish": [
    {
      "provider": "github",
      "owner": "614146239",
      "repo": "electron-camere",
      "releaseType": "release"
    }
  ]
}
